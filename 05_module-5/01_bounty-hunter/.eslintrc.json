{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "prefer-destructuring": [
      "error",
      {
        "VariableDeclarator": {
          "object": true
        }
      }
    ]
  }
}